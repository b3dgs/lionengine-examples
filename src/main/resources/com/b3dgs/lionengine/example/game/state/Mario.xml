<?xml version="1.0" encoding="UTF-8"?>
<lionengine:object xmlns:lionengine="http://lionengine.b3dgs.com">
    <lionengine:class>com.b3dgs.lionengine.example.game.state.Mario</lionengine:class>
    <lionengine:surface image="Mario.png"/>
    <lionengine:frames horizontal="7" vertical="1"/>
    <lionengine:size width="16" height="16"/>
    <lionengine:animation name="idle" start="1" end="1" step="1" speed="0.125" reversed="false" repeat="false"/>
    <lionengine:animation name="walk" start="4" end="6" step="1" speed="0.25" reversed="false" repeat="true"/>
    <lionengine:animation name="turn" start="7" end="7" step="1" speed="0.125" reversed="false" repeat="false"/>
    <lionengine:animation name="jump" start="3" end="3" step="1" speed="0.25" reversed="false" repeat="false"/>
</lionengine:object>
